{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Azure.WebJobs": "Information"
    }
  },
  "DurableTask": {
    "HubName": "WorkflowHub"
  },
  "OpenTelemetry": {
    "MetricExporters": "AzureMonitor",
    "TraceExporters": "AzureMonitor",
    "LogExporters": "AzureMonitor",
    "InstrumentOrleans": true,
    "InstrumentAspNetCore": true,
    "InstrumentHttpClient": true,
    "SamplingRate": 0.01
  }
}
